{
  "name": "react-router-rag",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build && node vercel/prepare.js",
    "deploy": "vercel",
    "dev": "cross-env NODE_ENV=development node ./dev-server.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "biome check .",
    "format": "biome format --write .",
    "lint:fix": "biome check --apply .",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:migrate": "supabase migration up",
    "db:reset": "supabase db reset",
    "db:link": "supabase link",
    "db:deploy": "supabase db push",
    "db:pull": "supabase db pull",
    "db:generate:types": "supabase gen types --local --schema public > app/db.types.ts",
    "db:generate:migration": "supabase migration new",
    "db:generate:seeds": "supabase db dump --data-only -f supabase/seed.sql"
  },
  "dependencies": {
    "@epic-web/client-hints": "^1.3.5",
    "@langchain/community": "^0.3.28",
    "@langchain/core": "^0.3.37",
    "@langchain/openai": "^0.4.2",
    "@langchain/textsplitters": "^0.1.0",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.5",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@react-router/express": "^7.1.5",
    "@react-router/node": "^7.1.5",
    "@tanstack/react-table": "^8.20.6",
    "@vercel/blob": "^0.27.1",
    "@vercel/node": "^3.2.25",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.0.4",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "crypto-js": "^4.2.0",
    "express": "^4.21.1",
    "isbot": "^5.1.17",
    "intl-parse-accept-language": "^1.0.0",
    "lucide-react": "^0.474.0",
    "pdf-parse": "^1.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "remix-utils": "^8.1.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.48.1",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@faker-js/faker": "^9.4.0",
    "@react-router/dev": "^7.1.5",
    "@tailwindcss/vite": "^4.0.3",
    "@types/express": "^5.0.0",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^12.0.1",
    "cross-env": "^7.0.3",
    "tailwindcss": "^4.0.3",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
