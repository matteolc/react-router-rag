{
	"name": "@matteolc/react-router-rag",
	"private": false,
	"author": "Matteo La Cognata",
	"type": "module",
	"scripts": {
		"bootstrap": "npm install && npm run db:start && npm run db:migrate && npm run db:generate:types",
		"build": "react-router build",
		"deploy": "vercel",
		"dev": "react-router dev",
		"start": "react-router-serve ./build/server/index.js",
		"typecheck": "react-router typegen && tsc",
		"lint": "biome check .",
		"format": "biome format --write .",
		"lint:fix": "biome check --apply .",
		"db:start": "supabase start",
		"db:stop": "supabase stop",
		"db:status": "supabase status",
		"db:migrate": "supabase migration up",
		"db:reset": "supabase db reset",
		"db:link": "supabase link",
		"db:deploy": "supabase db push",
		"db:pull": "supabase db pull",
		"db:generate:types": "supabase gen types --local --schema public > app/db.types.ts",
		"db:generate:migration": "supabase migration new",
		"db:generate:seeds": "supabase db dump --data-only -f supabase/seed.sql"
	},
